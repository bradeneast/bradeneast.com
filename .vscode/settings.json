{
	"netlify.site_id": "a398d413-5c2c-430f-8318-8a3add053a22",
	"liveSassCompile.settings.formats": [
		{
			"format": "compressed",
			"extensionName": ".css",
			"savePath": "/dist/_assets/css"
		}
	],
	"liveServer.settings.root": "/dist",
	"liveServer.settings.file": "/404/index.html",
	"saveAndRun": {
		"commands": [
			{
				"cmd": "esbuild --target=es6 --bundle --minify ./src/_js/main.js --outfile=./dist/_assets/js/main.js",
				"match": ".*src.*js",
				"silent": true,
			},
			{
				"cmd": "esbuild --target=es6 --bundle --minify ./src/_js/search.js --outfile=./dist/_assets/js/search.js",
				"match": ".*src.*js",
				"silent": true,
			},
			{
				"cmd": "esbuild --target=es6 --bundle --minify ./src/_js/codeDemo.js --outfile=./dist/_assets/js/codeDemo.js",
				"match": ".*src.*js",
				"silent": true,
			},
		]
	},
}